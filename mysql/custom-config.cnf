[mysqld]
innodb_file_per_table = 1
innodb_file_format = barracuda
innodb_buffer_pool_size = 800M
innodb_log_file_size = 512M
key_buffer              = 36M
max_allowed_packet      = 250M
thread_stack            = 192K
thread_cache_size       = 8
innodb_lock_wait_timeout = 180
innodb_flush_log_at_trx_commit = 0
innodb_support_xa = 0
